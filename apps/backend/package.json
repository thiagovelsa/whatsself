{
  "name": "@whats-self/backend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "node scripts/run-dev-windows.mjs",
    "dev:watch": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build:check": "tsc -p tsconfig.json --noEmit",
    "start": "node dist/index.js",
    "start:prod": "set NODE_ENV=production && node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "setup:test-db": "node scripts/setup-test-db.js",
    "test": "npm run setup:test-db && vitest run src/__tests__/businessRules.test.ts && vitest run src/__tests__/circuitBreaker.test.ts && vitest run src/__tests__/authService.test.ts && vitest run src/__tests__/config.test.ts && vitest run src/__tests__/api.test.ts",
    "test:watch": "npm run setup:test-db && vitest",
    "test:ui": "npm run setup:test-db && vitest --ui",
    "test:coverage": "npm run setup:test-db && vitest run --coverage",
    "config:init": "node --import tsx scripts/init-config.mjs",
    "config:sync": "node --import tsx scripts/sync-config.mjs",
    "admin:reset": "node --import tsx scripts/reset-admin-credentials.mjs",
    "test:ws": "node scripts/test-websocket.mjs",
    "lint": "echo 'Linting not configured yet'",
    "format": "echo 'Formatting not configured yet'",
    "clean": "if exist dist rmdir /s /q dist",
    "clean:all": "npm run clean && if exist node_modules rmdir /s /q node_modules"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.4.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.8.1",
    "whatsapp-web.js": "^1.34.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.8",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.5.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3",
    "vitest": "^4.0.8"
  }
}
